{
    "name": "tbdscripts/laravel-achievements",
    "description": "Simple, elegant Achievements the Laravel way",
    "keywords": [
        "php",
        "laravel",
        "package",
        "achievements",
        "badges",
        "tehwave"
    ],
    "homepage": "https://github.com/tehwave/laravel-achievements",
    "license": "MIT",
    "authors": [
        {
            "name": "Peter JÃ¸rgensen",
            "email": "peterchrjoergensen@gmail.com",
            "homepage": "https://peterchrjoergensen.dk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3",
        "illuminate/console": "^8.0",
        "illuminate/database": "^8.0",
        "illuminate/filesystem": "^8.0",
        "illuminate/support": "^8.0",
        "ramsey/uuid": "^4.1.1"
    },
    "require-dev": {
        "orchestra/testbench": "^v6.3.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "tehwave\\Achievements\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tehwave\\Achievements\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "extra": {
        "laravel": {
            "providers": [
                "tehwave\\Achievements\\AchievementsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
